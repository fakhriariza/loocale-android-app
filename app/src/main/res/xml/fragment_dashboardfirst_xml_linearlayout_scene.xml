<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_editor_absoluteY="238dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="-17dp"
            android:layout_width="@dimen/_350sdp"
            android:id="@+id/tv_boardingTitle" />
        <Constraint
            motion:layout_editor_absoluteY="180dp"
            android:layout_height="@dimen/_43sdp"
            motion:layout_editor_absoluteX="183dp"
            android:layout_width="@dimen/_60sdp"
            android:id="@+id/iv_boardingLogo" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>